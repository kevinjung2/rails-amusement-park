<%= @attraction.name %>

<%= link_to "Edit Attraction", edit_attraction_path(@attraction) if @user.admin %>

<%= form_for :ride, url: '/rides' do |f| %>
  <%= f.hidden_field :attraction_id, value: @attraction.id %>
  <%= f.hidden_field :user_id, value: @user.id %>
  <%= f.submit 'Go on this ride' %>
<% end %>
